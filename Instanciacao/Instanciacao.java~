public class Instanciacao {
	public static void main(String[] args) {
	
		Automovel carro = new Automovel();;

		carro.setMotor("V6");
		carro.setChassi("9BWHE21JX24060960");
		carro.setPlaca("BRA2E19");
		carro.setMarca("Porsche");
		carro.setModelo("Boxster");
		carro.setCor("Branco");
		carro.setAno(2021);
		carro.setCombustivel(90.6);
		carro.setVelocidade(60.3);
		carro.setLigado(true);

		System.out.println(carro.getPlaca()+" "+carro.getMarca()+" "+carro.getModelo()+" "+carro.getCor()+" "+carro.getAno()+"\nFim!");
	}
}
